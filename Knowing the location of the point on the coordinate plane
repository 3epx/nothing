#include <iostream>
using namespace std;
int main() {
    int x = 0 , y = 0;
    cout << "enter the values in paralell : ";
    cin >> x >> y;
    
    if (x == 0 and y == 0){cout << 0 << endl;}
    else if (0 == y and x > 0 or  x < 0){cout << 1 << endl; }
    else if (0 == x and y > 0 or  y < 0){cout << 2 << endl; }
    else if (x > 0 and y > 0 or y < 0 and x < 0 or y < 0 and x > 0 or y >0 and x < 0 or x == y){cout << 3 << endl; }


    return 0;
}
