#include <iostream>
using namespace std;
void prevhis(int m , int d){

    if (m == 1 and d == 31){
    cout << "the month is : " << 2 << " and day is : " << 1;
    }
    else if  (m == 2 and d == 28){
    cout << "the month is : " << 3 << " and day is : " << 1;
    }
    else if  (m == 3 and d == 31){
    cout << "the month is : " << 4 << " and day is : " << 1;
    }
    else if  (m == 4 and d == 30){
    cout << "the month is : " << 5 << " and day is : " << 1;
    }
    else if  (m == 5 and d == 31){
    cout << "the month is : " << 6 << " and day is : " << 1;
    }
    else if  (m == 6 and d == 30){
    cout << "the month is : " << 7 << " and day is : " << 1;
    }
    else if  (m == 7 and d == 31){
    cout << "the month is : " << 8 << " and day is : " << 1;
    }
    else if  (m == 8 and d == 31){
    cout << "the month is : " << 9 << " and day is : " << 1;
    }
    else if  (m == 9 and d == 30){
    cout << "the month is : " << 10 << " and day is : " << 1;
    }
    else if  (m == 10 and d == 31){
    cout << "the month is : " << 11 << " and day is : " << 1;
    }
    else if  (m == 11 and d == 30){
    cout << "the month is : " << 12 << " and day is : " << 1;
    }
    else if  (m == 12 and d == 31){
    cout << "the month is : " << 1 << " and day is : " << 1;
    }
    else {
        cout << "the month is : " << m << " and day is : " << d +1;
    }
}
int main() {

int m = 0 , d = 0;
cout << "enter the month pls : ";
cin >> m;
cout << "enter the day pls : ";
cin >> d;

prevhis(m , d);


    return 0;
}
