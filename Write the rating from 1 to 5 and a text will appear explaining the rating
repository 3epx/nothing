#include <iostream>
using namespace std;
int main() {

int x = 0;
cout << "enter the number from 1 to 5 : ";
cin >> x;

if (x == 1){cout << "bad";}
else if (x == 2){cout << "unsatisfactory";}
else if (x == 3){cout << "mediocre";}
else if (x == 4){cout << "good";}
else if (x == 5){cout << "excellent";}
else {cout << "worng !!!!";}


    return 0;
}
