#include <iostream>
using namespace std;
int main() {
int x[10] = {1,2,2,2,5,63,63,8,9,10};
int y[10];
for (int i = 0 , u = 9; i <= 9 and u >=0 ; i++ , u--){
y[i] = x[u];
}
cout << "old array is : ";
for (int i = 0; i <10; i++){
    cout << x[i] << " ";
}
cout << endl << "new array is : ";
for (int i = 0; i <10; i++){
    cout << y[i] << " ";
}
    return 0;
}
