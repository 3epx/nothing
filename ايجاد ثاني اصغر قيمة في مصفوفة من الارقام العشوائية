#include <iostream>
using namespace std;

int main() {

int x[10]={131,2,3,134,415,216,147,128,91,1210};
int min = x[0];
for(int i = 0; i<10; i++){
    
    if (min > x[i]){
        min = x[i];
    }
}
int i = 0;
int z = min;
while(true){
    z = z + 1;
    for (i = 0; i<10; i++){
        if(z == x[i]){
            cout <<"the second smallest number is : " << x[i] << endl;
            break;
        }
    }
    if(z == x[i]){break;}
}
    return 0;
}
