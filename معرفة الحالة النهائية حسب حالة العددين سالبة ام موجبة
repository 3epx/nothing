#include <iostream>
using namespace std;

void ston(int a , int b){
    
    if (a > 0){a = 1;}
    else if (a < 0 ){a = -1;}
    else {a = 0;}
    if (b > 0){b = 1;}
    else if (b < 0){b = -1;}
    else {b = 0;}
    
    int result = a + b;
    cout << result;
}
int main() {

int a , b ;
cout << "enter the value of a : ";
cin >> a;
cout << "enter the value of b : ";
cin >> b;

ston(a,b);

    return 0;
}
