#include <iostream>
#include <cmath>
using namespace std;

void power2(int a , int &p1 , int &p2, int &p3){
    p1 = pow(a,p1);
    p2 = pow(a,p2);
    p3 = pow(a,p3);
}
int main() {
    
    int a = 0, p1 = 0 , p2 = 0 , p3 = 0;
    cout << "enter the number : ";
    cin >> a;
    cout << "enter the powers : ";
    cin >> p1 >> p2 >> p3;
    power2(a,p1,p2,p3);
    cout << "the values is : "<< endl <<
    p1 << endl<<
    p2 << endl<< 
    p3;
    return 0;
}
