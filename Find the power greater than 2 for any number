#include <iostream>
using namespace std; 
int main() {

int a = 0 , n = 0 , sum = 1;
  cout << "enter the number pls : ";
  cin >> a;
  cout << "enter the value of power : ";
  cin >> n;
  cout << endl;
for (int i = 1; i <= n ; i++){
    sum = sum * a; 
}
cout << "final result is : " << sum;


    return 0;
}
