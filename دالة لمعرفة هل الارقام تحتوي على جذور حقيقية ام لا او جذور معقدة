#include <iostream>
using namespace std; 

void root(int a  , int b , int c){
    int d = c * a * 4 - b*b;
    if (d > 0){cout << "There are two real and different roots";}
    else if (d == 0){cout << "There is a double real root";}
    else if (d < 0){cout << "There are no real roots";}
}

int main() {

int a = 0 , b = 0 , c = 0; 
cout << "enter the number pls : ";
cin >> a >> b >> c;
root(a,b,c);

    return 0;
}
