#include <iostream>
using namespace std;
int main() {
    
   int n1 = 0 , n2 = 0 , n3 = 0;
   cout << "enter the numbers pls : ";
   cin >> n1 >> n2 >> n3;
   
   if (n1 >= 1 and n2 >= 1 and n3 >=1){cout << "positive numbers is : 3";}
   else if (n1 < 0 and n2 >= 1 and n3 >=1){cout << "positive numbers is : 2";}
   else if (n1 >= 1 and n2 >= 1 and n3 < 0){cout << "positive numbers is : 2";}
   else if (n1 >= 1 and n2 < 0 and n3 >= 1){cout << "positive numbers is : 2";}
   else if (n1 < 0 and n2 < 0 or n3 >= 1){cout << "positive numbers is : 1";}
   else if (n1 < 0 and n2 >= 1 or n3 < 0){cout << "positive numbers is : 1";}
   else if (n1 < 0 and n2 < 0 or n3 >= 1){cout << "positive numbers is : 1";}
   
   
    return 0;
}
