#include <iostream>
using namespace std;

int main() {

string password = "good_dev";
string in_pass;
cout << "enter the password : ";
cin >> in_pass;
if(in_pass == password){cout << "login succusfuly";}
while (in_pass != password){
    cout << "the password is worng pls agine : ";
    cin >> in_pass;
    if(in_pass == password){cout << "login succusfuly";}
}

    return 0;
}
