#include <iostream>
#include <cmath>
using namespace std;

class sq{
    private : 
    int x1 , x2 , y1 , y2;
    public:
    sq(int x1, int x2, int y1 , int y2){
        this -> x1 = x1;
        this -> x2 = x2;
        this -> y1 = y1;
        this -> y2 = y2;
    }
     void pr(){
        int z = pow((x1 -x2) , 2) + pow((y1 -y2) , 2);
        z = sqrt(z);
        cout << "the distance between two points is : " << z << endl;
    }
    
};


int main() {
sq s1(3,8,2,4);s1.pr();
sq s2(52,8,22,4);s2.pr();
sq s3(3,83,32,4);s3.pr();
sq s4(32,8,22,44);s4.pr();
sq s5(43,83,2,422);s5.pr();


    return 0;
}
