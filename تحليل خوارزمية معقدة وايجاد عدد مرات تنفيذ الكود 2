        for (int i = 2; i <= n * n; i++) {
                  for (int k = i; k <= n; k++) {
                        for (int d = 1; d <=i-1; d *= 2) {
                               cout << "ali" << endl;             
             }
         }
     }



الحل 


#include <iostream>
  #include <cmath>
  using namespace std;
  int main() {
      int n = 69;
      int sum = 0;
      for (int i = 2; i <= n; i++) {
          int multi = floor((log(i - 1) / log(2)) + 1) * (n - i + 1);
          sum = sum + multi;
         }
     return 0;
     }

