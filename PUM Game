#include <iostream>
using namespace std;
int main()
{
    
  int number = 0 , ncu = 0; 
  
  cout << "enter the number pls : ";
  cin >> number;
  
  for (int i = 0; i <= number; i++){
      
      for (int u = 0; u <= 3 ; u++){
          cout << ncu << " ";
          ncu++;
      }
      
      cout << " PUM" << endl;
  }
  
  
  
    return 0;
}
