#include <iostream>
using namespace std;
int main() {

int n = 0 , check = 0;
cout << "enter the number pls : ";
cin >> n;

for (int i = 1; i <= n; i++){
        if (n %i == 0){check++;}
  
}
  if (check == 2){cout << "the number is prime" << endl;}
  else {cout << "this number is not prime";}
    return 0;
}
