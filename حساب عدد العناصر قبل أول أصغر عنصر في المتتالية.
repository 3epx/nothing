#include <iostream>
using namespace std;
int main() {

	float nums[10] = {3, 0.2, 2.9,8,1,3.99,8,5,8,1 };
	float min = nums[0];
	int i_p_min = 0;
	for (int i = 0; i < sizeof(nums) / sizeof(nums[0]);i++) {
		if (nums[i] < min) {
			min = nums[i];
			i_p_min = i;
		}
	}
	if (i_p_min > 0) {
		cout << i_p_min;
	}

}
