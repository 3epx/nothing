#include <iostream>
#include <cmath>
using namespace std; 
int main() {

int a = 0 , n = 0;
  cout << "enter the number pls : ";
  cin >> a;
  cout << "enter the value of power : ";
  cin >> n;
  cout << endl;
for (int i = 1; i <= n ; i++){
    cout << pow(a , i);
    cout << endl;
}


    return 0;
}
