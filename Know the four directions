if(n1 == 1 and n2 == 1){cout << "east";}
else if (n1 == 1 and n2 == 2){cout << "west";}
else if (n1 == 1 and n2 == 3){cout << "south";}
else if (n1 == 2 and n2 == 1){cout << "west";}
else if (n1 == 2 and n2 == 2){cout << "east";}
else if (n1 == 2 and n2 == 3){cout << "north";}
else if (n1 == 3 and n2 == 1){cout << "south";}
else if (n1 == 3 and n2 == 2){cout << "north";}
else if (n1 == 3 and n2 == 3){cout << "west";}
else if (n1 == 4 and n2 == 1){cout << "north";}
