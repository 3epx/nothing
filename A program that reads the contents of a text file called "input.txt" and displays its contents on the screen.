#include <iostream>
#include <fstream>
#include <string>
using namespace std;
int main() {
    string read;
    ifstream file;
    file.open(R"(C:\Users\Ahmed\Desktop\file handling test\text.txt)");
    while (getline(file, read)) {
        cout << read << endl;
    }
    file.close();


    return 0;
}
