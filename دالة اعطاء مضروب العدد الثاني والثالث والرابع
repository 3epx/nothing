#include <iostream>
using namespace std;

int pow3(int a , int &b , int &c , int &d){
    b = a*a;
    c = a*a*a;
    d = a*a*a*a;
    return 0;
}
int main() {

int a = 0;
int b = 0;
int c = 0;
int d = 0;

cout << "enter the number pls : ";
cin >> a;
int z = pow3(a,b,c,d);
cout << "final result is : " << b <<" and " << c << " and " << d;

    return 0;
}
