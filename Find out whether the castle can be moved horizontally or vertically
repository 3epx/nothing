#include <iostream>
using namespace std;
int main() {
    
    int  x = 0 , y = 0 , x1 = 0 , y1 = 0;
    cout << "enter the coordinates of first squre : "; 
    cin >> x >> y;
    cout << "enter the coordinates of second squre : "; 
    cin >> x1 >> y1;
    bool result = (x == x1) or (y == y1); 
    cout << result;
    return 0;
}
