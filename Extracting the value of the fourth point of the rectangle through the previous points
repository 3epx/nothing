#include <iostream>
using namespace std;
int main() {
    int x1 = 0 , x2 = 0 , x3 = 0 , y1 = 0 , y2 =0 , y3 = 0 , x4 = 0 , y4 = 0;
    cout << "enter the values of x cordinates in paralell : ";
    cin >> x1 >> x2 >> x3;
    cout << "enter the values of y cordinates in paralell : ";
    cin >> y1 >> y2 >> y3;
    
    if (x1 == x2){x4 = x1 + x2 + 1;}
    else if (x1 == x3){x4 == x1 + x3 + 1;}
    else if (x2 == x3){x4 == x2 + x3 + 1;}

    if (y1 == y2){y4 = y1 + y2 + 1;}
    else if (y1 == y3){x4 == y1 + y3 + 1;}
    else if (y2 == y3){x4 == y2 + y3 + 1;}
    
cout << "the value of x4 is : " << x4 << " and y4 is : " << y4;


    return 0;
}
