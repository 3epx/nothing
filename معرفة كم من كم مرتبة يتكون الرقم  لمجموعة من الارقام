#include <iostream>
using namespace std;
void hmn(int n){
    int hmn = 0;
    int k = n;
    for (;k >0;){
        k = k / 10;
        hmn++;
    }
    cout << n <<  " has " << hmn << " digts" << endl;
}
int main() {

int nums[5];
for (int i = 1; i<= 5;i++){
    cout << "enter the number pls : ";
    cin >> nums[i];
}
for (int i = 1; i<= 5; i++){
    hmn(nums[i]);
}
    return 0;
}
