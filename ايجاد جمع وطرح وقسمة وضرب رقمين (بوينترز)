#include <iostream>
using namespace std;
int main() {

int x = 19 , y = 20;
int *x1 = &x , **x2 = &x1;
int *y1 = &y , **y2 = &y1;

cout << "the sum of the numbers is : " << **x2 + **y2 << endl;
cout << "the multiply of the numbers is : " << **x2 * **y2 << endl;
cout << "the devide of the numbers is : " << **x2 / **y2 << endl;
cout << "the sub of the numbers is : " << **x2 - **y2 << endl;

    return 0;
}
