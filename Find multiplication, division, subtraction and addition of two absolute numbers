#include <iostream>
#include <cmath>
using namespace std;
int main() {
    int a = 0 , b = 0 , sum = 0;
    cout << "enter the numbers : ";
    cin >> a >> b;
    sum = a + b;
    if (sum < 0){sum  = sum * -1;}
    cout<< sum << endl;
    sum = a - b;
    if (sum < 0){sum  = sum* -1;}
    cout << sum << endl;
    sum = a * b;
    if (sum < 0){sum = sum * -1;}
    cout << sum << endl;
    sum = a / b;
    if (sum < 0){sum = sum* -1;}
    cout << sum;
    return 0;
}
