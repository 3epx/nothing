#include <iostream>
using namespace std;
void sort(int a , int b , int c){
    if (a <= b and a <= c){cout << "smaller number is : " << a;}
    else if (b <= a and b <= c){cout << "smaller number is : " << b;}
    else if (c <= a and c <= b){cout << "smaller number is : " << c;}
    cout << endl;
    if (a >= b and a <= c or a <= b and a >= c){cout << "midal number is : " << a;}
    else if (b >= a and b <= c or b <= a and b >= c){cout << "midal number is : " << b;}
    else if (c >= a and c <= b or c <= a and c >= b){
        cout << "midal number is : " << c;
    }
    cout << endl;
    if (a >= b and a >= c or a >= b and a >= c){
        cout << "bigger number is : " << a;
    }
    else if (b >= a and b >= c or b >= a and b >= c){
        cout << "bigger number is : " << b;
    }
    else if (c >= a and c >= b or c >= a and c >= b){
        cout << "bigger number is : " << c;
    }   
    
}

int main() {

int a = 0 , b = 0 , c = 0;
cout << "enter the values pls : ";
cin >> a >> b >> c;
sort(a,b,c);

    return 0;
}
