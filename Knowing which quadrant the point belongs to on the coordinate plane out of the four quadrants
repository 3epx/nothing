#include <iostream>
using namespace std;
int main() {
    int x = 0 , y = 0;
    cout << "enter the values in paralell : ";
    cin >> x >> y;
    
    if(x,y == 0){cout << "The point is located in the center";}
    else if(x > 0 or x < 0 and y == 0){cout << "The point lies on the X axis";}
    else if(y > 0 or y < 0 and x == 0){cout << "The point lies on the Y axis";}
    else if(x > 0 and y > 0){cout << "The point is located in the first quadrant";}
    else if(x < 0 and y > 0){cout << "The point is located in the second quadrant";}
    else if(x < 0 and y < 0){cout << "The point is located in the third quadrant";}
    else if(x > 0 and y < 0){cout << "The point is located in the fourd quadrant";}
    else {cout << "worng";}


    return 0;
}
