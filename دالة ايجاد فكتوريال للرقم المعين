#include <iostream>
using namespace std;

void fact(int n){
    int sum = 1;
    for(int i = 1; i <= 5; i++ ){
        sum = sum * i;
    }
    cout << "factorial of " << n << " = " << sum;
}
int main() {
    
    int n = 0; 
    cout << "enter the number pls : ";
    cin >> n;
    fact(n);
    return 0;
}
