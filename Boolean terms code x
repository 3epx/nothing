#include <iostream>
using namespace std;
int main() {
    
    int a = 0 , b = 0 , c = 0; 
    cout << "enter the numbers pls : ";
    cin >> a >> b >> c;
    
    bool result = (a > 0) and (b < 0) and (c < 0) || (a < 0) and (b > 0) and (c > 0) || (a < 0) and (b < 0) and (c > 0); 
    cout << result; 
    
    
    return 0;
}
