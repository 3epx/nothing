#include <iostream>
using namespace std;
int main() {
    int a = 0 , b = 0 , c = 0;
    cout << "enter the numbers  : ";
    cin >> a >> b >> c;
    cout << "the volume of rectangular prism is  : " << a * b * c << endl;
    cout << "the surface area of a rectangular prism is  : " << 2 * (a*b + b*c + a*c);
    return 0;
}
