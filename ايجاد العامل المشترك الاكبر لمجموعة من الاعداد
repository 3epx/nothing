#include <iostream>
#include <cmath>
using namespace std;
void gcd(int a , int b){
    int k = 0;
    if (a > b){
        for (int i = 1; i<= b;i++){
            if (b %i == 0 and a %i == 0){k = i;}
        }
    }else if (a < b){
        for (int i = 1; i<= a;i++){
            if (b %i == 0 and a %i == 0){k = i;}
        }        
    }
    cout << "GCD is : "<<k;
}
int main() {
int a = 0, b= 0 , c = 0 , d = 0;
cout << "enter the first number : ";
cin >> a;
cout << "enter the second number : ";
cin >> b;
cout << "enter the third number : ";
cin >> c;
cout << "enter the fourth number : ";
cin >> d;
gcd(a,b);
cout << endl;
gcd(a,c);
cout << endl;
gcd(a,d);

    return 0;
}
