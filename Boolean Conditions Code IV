#include <iostream>
using namespace std;
int main() {
    
    int a = 0, b = 0 , c = 0;
    cout << "enter the number in paralell a then b then c : ";
    cin >> a >> b >> c;
    bool result = (b >= a) && (b<= c) or (b <= a) && (b >= c);
    cout << "the number is : " << result;
    
    return 0;
}
