set1 = {1 , 2, 3 , 4 , 5}
set2 = {5 , 6 , 7 , 8 ,9 ,10}

set3 = set1.union(set2) # اتحاد المجموعتين
print(set3)

set3 = set1.intersection(set2) # تقاطع المجموعتين
print(set3)

set3 = set1.difference(set2) # الفرق بين المجموعتين
print(set3)

set3 = set1.symmetric_difference(set2) # الفرق المتماثل بين المجموعتين
print(set3)
