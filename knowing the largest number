#include <iostream>
using namespace std;

int main() {

int n = 0, big_n = 0 , num = 0;
cout << "enter the how many numbers pls :";
cin >> n;
for(int i = 0; i< n; i++){
    cout << "enter the number pls :";
    cin >> num;
    if (num > big_n){big_n = num;}
}
cout << "the largest number is : " << big_n;
    return 0;
}
