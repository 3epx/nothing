#include <iostream>
using namespace std;

void sum(int( &x)[10]){
    int hmn = 0;
    for(int i = 0; i<10; i++){
        for(int u = 0; u<10 ; u++){
            if (x[i] == x[u]){hmn++;}
        }
        for (int u = 0; u<i ; u++){
            if (x[i] == x[u]){hmn = 0;}
        }
        if (hmn > 0){cout << x[i] << " has : " <<hmn << endl;}
        hmn = 0;
    }
};

int main() {

int x[10] = {1,2,2,8,5,8,1,8,9,10};
sum(x);

    return 0;
}
