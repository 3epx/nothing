#include <iostream>
using namespace std;

int main() {
    char input;

    cout << "Enter a character: ";
    cin >> input;

    if (input >= 'A' && input <= 'Z') {
        cout << input << " is an uppercase letter." << endl;
    }
    else if (input >= 'a' && input <= 'z') {
        cout << input << " is a lowercase letter." << endl;
    }
    else if (input >= '0' && input <= '9') {
        cout << input << " is a digit." << endl;
    }
    else {
        cout << input << " is neither a letter nor a digit." << endl;
    }

    return 0;
}
