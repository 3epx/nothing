#include <iostream>
using namespace std;
int isprime(int n){
    int hmn = 0;
    for (;n>0;){
        n = n / 10;
        hmn ++;
    }
    return hmn;


}
int main() {
for (int i = 1; i <= 10; i++ ){
    int n = 0;
    cout << "enter the number pls :";
    cin >> n; 
    int z = isprime(n);
    cout << z << endl;
}
    return 0;
}
