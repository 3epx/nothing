#include <iostream>
#include <cmath>
using namespace std;
void witd(int n , int d){
    int z = pow(10,d);
    n = n%z;
    z = z / 10;
    n = n / z;
    cout << n;
}
int main() {

int n = 0 , d = 0;
cout << "enter the number pls : ";
cin >> n;
cout << "enter the number of digt pls : ";
cin >> d;
witd(n,d);
    return 0;
}
